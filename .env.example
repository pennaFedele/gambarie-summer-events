# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Cloudflare Turnstile
VITE_TURNSTILE_SITE_KEY=1x00000000000000000000AA

# Enable/disable CAPTCHA (set to 'false' to disable, defaults to enabled)
VITE_CAPTCHA_ENABLED=true

# Get your site key from: https://dash.cloudflare.com/
# For testing, you can use: 1x00000000000000000000AA (always passes)
# For failure testing: 2x00000000000000000000AB (always fails)
# For interactive testing: 3x00000000000000000000FF (requires user interaction)

# Umami Analytics (optional)
# Replace with your own Umami analytics configuration
VITE_UMAMI_SCRIPT_URL=https://your-umami-instance.com/script.js
VITE_UMAMI_WEBSITE_ID=your-website-id